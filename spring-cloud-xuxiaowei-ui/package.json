{
  "name": "spring-cloud-xuxiaowei-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "pre:npm": "npm install --registry https://registry.npmmirror.com || npm install --registry https://registry.npmjs.org",
    "pre:yarn": "yarn install --registry https://registry.npmmirror.com || yarn install --registry https://registry.npmjs.org",
    "pre:pnpm": "pnpm install --registry https://registry.npmmirror.com || pnpm install --registry https://registry.npmjs.org",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "author": "徐晓伟 <xuxiaowei@xuxiaowei.com.cn>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/xuxiaowei-cloud/spring-cloud-xuxiaowei/issues"
  },
  "homepage": "https://github.com/xuxiaowei-cloud/spring-cloud-xuxiaowei#README",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuxiaowei-cloud/spring-cloud-xuxiaowei.git"
  },
  "keywords": [
    "pinia",
    "typescript",
    "vite",
    "vue",
    "vue-router"
  ],
  "dependencies": {
    "pinia": "2.1.7",
    "vue": "3.4.21",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.9.0",
    "@tsconfig/node20": "20.1.4",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.30",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.5",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.24.0",
    "jsdom": "24.0.0",
    "npm-run-all2": "6.1.2",
    "prettier": "3.2.5",
    "typescript": "5.4.3",
    "vite": "5.2.6",
    "vite-plugin-vue-devtools": "7.0.25",
    "vitest": "1.4.0",
    "vue-tsc": "2.0.7"
  }
}
